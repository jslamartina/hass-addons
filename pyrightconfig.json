{
  "enableReachabilityAnalysis": true,
  "include": ["."],
  "exclude": [
    ".venv",
    "node_modules",
    "src/cync_controller/mitm/validate-checksum-REFERENCE-ONLY.py"
  ],
  "venvPath": ".",
  "venv": ".venv",
  "pythonVersion": "3.14",
  "pythonPlatform": "Linux",
  "useLibraryCodeForTypes": true,
  "executionEnvironments": [
    {
      "root": "src",
      "pythonVersion": "3.14",
      "reportPrivateUsage": true
    },
    {
      "root": "tests",
      "extraPaths": ["src", "scripts"],
      "pythonVersion": "3.14",
      "reportPrivateUsage": false
    },
    {
      "root": "scripts",
      "extraPaths": ["src", "tests"],
      "pythonVersion": "3.14",
      "reportPrivateUsage": false
    }
  ]
}
