{
  "include": ["src"],
  "exclude": [
    "tests",
    "mitm",
    "mitm/validate-checksum-REFERENCE-ONLY.py"
  ],
  "extraPaths": ["src", "."],
  "venvPath": ".",
  "venv": ".venv",
  "pythonVersion": "3.14",
  "typeCheckingMode": "strict",
  "useLibraryCodeForTypes": true,
  "pythonPlatform": "Linux",
  "executionEnvironments": [
    {
      "root": ".",
      "extraPaths": ["src", "."],
      "pythonVersion": "3.14"
    }
  ],
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "stubPath": "typings"
}
