{
  "extends": "../pyrightconfig.json",
  "include": ["src", "tests", "mitm"],
  "exclude": ["mitm/validate-checksum-REFERENCE-ONLY.py"],
  "extraPaths": ["src", "mitm", "."],
  "venvPath": "..",
  "venv": ".venv",
  "pythonVersion": "3.14",
  "pythonPlatform": "Linux",
  "executionEnvironments": [
    {
      "root": "src",
      "extraPaths": ["src", "."],
      "pythonVersion": "3.14",
      "reportPrivateUsage": true
    },
    {
      "root": "tests",
      "extraPaths": ["src", "."],
      "pythonVersion": "3.14",
      "reportPrivateUsage": false
    },
    {
      "root": "mitm",
      "extraPaths": ["src", "."],
      "pythonVersion": "3.14",
      "reportPrivateUsage": false
    }
  ]
}
