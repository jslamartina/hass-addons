{
  "typeCheckingMode": "strict",
  "include": ["."],
  "extraPaths": ["src", "../scripts/playwright", "typings"],
  "venvPath": "..",
  "venv": ".venv",
  "useLibraryCodeForTypes": true,
  "reportMissingImports": "error",
  "reportPrivateUsage": "error",
  "stubPath": "typings",
  "executionEnvironments": [
    {
      "root": ".",
      "extraPaths": ["src", "../scripts/playwright", "typings"]
    }
  ]
}
