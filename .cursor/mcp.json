{
  "mcpServers": {
    "web_fetch": {
      "command": "python",
      "args": ["-m", "mcp_server_fetch"]
    },
    "docker": {
      "command": "uvx",
      "args": ["mcp-server-docker"]
    },
    "git": {
      "command": "python",
      "args": ["-m", "mcp_server_git"]
    },
    "python": {
      "command": "uvx",
      "args": ["mcp-run-python", "stdio"]
    },
    "time": {
      "command": "python",
      "args": ["-m", "mcp_server_time", "--local-timezone=America/Chicago"]
    }
  }
}
